{
  "id": "memory.overcommit_high.v1",
  "name": "High Memory Overcommit",
  "description": "Detect sustained high Memory Overcommit (1 Minute Avg).",
  "enabled": true,
  "severity": "high",
  "detector": {
    "type": "threshold_sustained",
    "comparison": "greater",
    "threshold": 100.0,
    "min_consecutive": 6,
    "filter": {
      "logic": "and",
      "conditions": [
        {"field": "attribute", "op": "eq", "value": "Memory: Memory Overcommit (1 Minute Avg)"}
      ]
    }
  }
}
