{
  "id": "numa.aggressive_migration.v1",
  "name": "Aggressive NUMA Migration Pattern",
  "description": "Detect zig-zag dominance between NUMA node processor load signals.",
  "enabled": true,
  "severity": "medium",
  "detector": {
    "type": "zigzag_switch",
    "min_switches": 6,
    "min_gap": 3.0,
    "filter": {
      "logic": "and",
      "conditions": [
        {"field": "object", "op": "eq", "value": "Numa Node"},
        {"field": "counter", "op": "eq", "value": "% Processor Time"}
      ]
    }
  }
}
